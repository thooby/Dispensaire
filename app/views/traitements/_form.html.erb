<%= form_for @traitement do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :groupe_traitement_id %><br />
   
    <%= f.collection_select(:groupe_traitement_id,GroupeTraitement.order("description"),:id,:description, :include_blank => true ) %>
  </p>
  <p>
    <%= f.label :acronime %><br />
    <%= f.text_field :acronime %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p><%= f.submit %></p>
<% end %>
